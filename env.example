# StyleSnap Environment Variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Edge Function for Real-time User Sync
# This function handles user creation synchronization
VITE_SUPABASE_SYNC_FUNCTION_URL=https://your-project-ref.supabase.co/functions/v1/sync-auth-users-realtime

# Cloudinary Configuration
# Get these from your Cloudinary dashboard: https://cloudinary.com/console
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here
VITE_CLOUDINARY_UPLOAD_PRESET=your_cloudinary_upload_preset_here

# Optional: Cloudinary API Key and Secret (for server-side operations)
# CLOUDINARY_API_KEY=your_api_key_here
# CLOUDINARY_API_SECRET=your_api_secret_here
